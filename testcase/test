int var_useless = 0;

int useless_loop()
{
	++var_useless;
	return 1;
}

int main()
{
	int i = 0;
	for (; i < 10; i = i + useless_loop())
		;

	int j = 0;
	for (; j + useless_loop() < 10; j = j + 1)
		;

	int k = 0;
	for (useless_loop(); k < 10; k = k + 1)
		;

	int l = 0;
	for (; l + var_useless++ < 100; l = l + 1)
		;

	int m = 0;
	for (; m < 200; m = m + var_useless++)
		;

	print(toString(var_useless));
	return 0;
}
