int[] a = new int[4];
int main()
{
    int[][] b = new int[4][];
	int i;
	b[0] = a;
	b[1] = a;
	b[2] = a;
	b[3] = a;
	println(toString(b.size()));
	b[0][0] = 1;
	b[0][1] = 2;
	b[0][2] = 3;
	b[0][3] = 4;
	for (i = 0; i < b[1].size(); i++)
		print(toString(b[1][i]));
	println("");
	for (i = 0; i < b[2].size(); i++)
		b[2][i] = 0;
	for (i = 0; i < b[3].size(); i++)
		print(toString(b[3][i]));
    return 0;
}